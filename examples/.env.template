# mollie-api-typescript SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: CLIENT_
#
# Security environment variables:
CLIENT_API_KEY=your_apikey_here
CLIENT_O_AUTH=your_oauth_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
CLIENT_DEBUG=true
